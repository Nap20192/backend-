  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Error Page</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      display: flex;
      justify-content: center;
      align-items: center;
      flex-direction: column;
      height: 100vh;
      margin: 0;
      background: #f8f9fa;
      color: #333;
      text-align: center;
    }

    h1 {
      font-size: 3rem;
      margin-bottom: 20px;
    }

    p {
      font-size: 1.5rem;
      margin-bottom: 30px;
      color: #555;
    }

    a {
      text-decoration: none;
      color: #000000;
      font-size: 1.2rem;
      font-weight: bold;
      border: 2px solid #ffffff;
      padding: 10px 20px;
      border-radius: 25px;
      transition: all 0.3s ease;
    }

    a:hover {
      background: #d2150e;
      color: #fff;
    }
